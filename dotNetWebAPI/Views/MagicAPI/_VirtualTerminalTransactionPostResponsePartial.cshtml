@model dotNetWebAPI.Models.VirtualTerminalTransactionPostResponseModels.VT_TRANSACTION
@using dotNetWebAPI.Models;


@if (Model != null)
{
    <hr>
    <div class="container">
        <div class="col-md-12" style="padding-left:0;">
            <div class="panel panel-primary">
                <div class="panel-heading">
                    <h3 class="panel-title">Virtual Terminal Post Response</h3>
                </div>
                <div class="panel-body">
                    <ul class="list-group">
                        <li class="list-group-item">
                            <strong class="">Status</strong> <div class="">@Model.status</div>

                        </li>
                        <li class="list-group-item">
                            <strong class="">Transaction Identifier</strong> <div class="">@Model.TRANSACTIONID</div>
                        </li>
                    </ul>
                    <small>Note: If you are seeing this, and not the magic ui page, there is a possiblity the redirect did not work properly. Please reference the help docs.</small>
                </div>
            </div>
        </div>
    </div>
}
